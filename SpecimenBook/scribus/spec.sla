            <ITEXT FONT="Seize Thin" FONTSIZE="7" CH="THE PERIODS OF DESIGN WHICH HAVE MOST AFFECTED PRINTING   43"/>
            <para/>
            <para/>
            <ITEXT FONT="Seize Thin" FONTSIZE="7" CH="SUPPLEMENTARY READING                                     64"/>
