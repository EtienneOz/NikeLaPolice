            <para/>
            <ITEXT FONT="Seize Thin" FONTSIZE="7" CH="  HAVE MOST STRONGLY INFLUENCED"/>
            <para/>
            <ITEXT FONT="Seize Thin" FONTSIZE="7" CH="  PRINTING"/>
            <para/>
            <para/>
            <ITEXT FONT="Seize Thin" FONTSIZE="7" CH="  BY"/>
