            <ITEXT FONT="Seize Thin" FONTSIZE="7" CH="TYPOGRAPHIC TECHNICAL SERIES FOR APPRENTICES--PART VII. NO. 43"/>
            <para/>
            <para/>
            <para/>
            <ITEXT FONT="Seize Thin" FONTSIZE="7" CH="  APPLIED DESIGN"/>
            <para/>
