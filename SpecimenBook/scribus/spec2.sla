            <para PARENT="HTML_default" LINESPMode="1"/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="(img, src: images/fig031bottom.jpg)"/>
            <para PARENT="HTML_default" LINESPMode="1"/>
            <para PARENT="HTML_default" LINESPMode="1"/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="Fig. 18. The diagram shows the motion of the eye as it perceives the"/>
            <para PARENT="HTML_default" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="design above. This motion is due to line entirely, not to accents of tone."/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH=" "/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" FEATURES="inherit underline" FCOLOR="Blue" CH="[Pg 32]"/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
