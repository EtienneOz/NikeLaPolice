            <para PARENT="HTML_default" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="taking the point of balance upon the line which divides"/>
            <para PARENT="HTML_default" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="the space in good proportion."/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH=" "/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="(img, src: images/fig027.jpg)"/>
