            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="Fig. 10. Placing a single line so that it will appear to be centered."/>
            <para PARENT="HTML_default" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="The dotted lines show the mathematical center of the vertical side."/>
            <para PARENT="HTML_default" LINESPMode="1"/>
