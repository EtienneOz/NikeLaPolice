            <para PARENT="HTML_default" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="margins through the light gray masses of type,"/>
            <para PARENT="HTML_default" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="to the darker grays of decorations."/>
