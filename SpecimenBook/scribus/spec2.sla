            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH=" "/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="On the printed page the eye may be definitely directed from one unit to another through this quality of motion, which forms a very valuable resource for the printer. "/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" FEATURES="inherit underline" FCOLOR="Blue" CH="Fig. 20"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH=" is a diagram of a simple use of motion, the eye progressing as indicated by the arrows through the masses which make up the page."/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH=" "/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" FEATURES="inherit underline" FCOLOR="Blue" CH="[Pg 33]"/>
            <para PARENT="Default Paragraph Style" LINESPMode="1"/>
            <ITEXT FONT="Seize2 Thin" FONTSIZE="5" CH="(img, src: images/fig033.jpg)"/>
